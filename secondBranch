#include <iostream>
#include <vector>
#include <algorithm> // Required for std::min_element

// Function to calculate the sum of elements in a vector
int getSum(const std::vector<int>& vec) {
    int sum = 0;
    for (int num : vec) {
        sum += num;
    }
    return sum;
}

// Function to find the minimum element in a vector
int getMin(const std::vector<int>& vec) {
    if (vec.empty()) {
        std::cerr << "Vector is empty. No minimum element." << std::endl;
        return 0; // Return a default value (you can choose a different approach if needed)
    }

    return *std::min_element(vec.begin(), vec.end());
}

int main() {
    std::vector<int> numbers = {10, 20, 30, 5, 40, 50};

    int sum = getSum(numbers);
    int min = getMin(numbers);

    std::cout << "Sum: " << sum << std::endl;
    std::cout << "Minimum: " << min << std::endl;

    return 0;
}
